{
  "name": "johnslist",
  "version": "1.0.0",
  "description": "An ecommerce site for selling broken items",
  "main": "./server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "cd client && npm run build",
    "seed": "node server/config/seeds.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlincb/johnslist.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carlincb/johnslist/issues"
  },
  "homepage": "https://github.com/carlincb/johnslist#readme",
  "dependencies": {
    "cloudinary-core": "^2.12.3",
    "concurrently": "^7.0.0",
    "graphql": "^15.5.0",
    "stripe": "^8.203.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^2.0.2"
  }
}
